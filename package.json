{
  "name": "comet-starter",
  "private": true,
  "version": "1.0.0",
  "description": "React and TypeScript starter app, pre-configured with the Comet component library and other tools.",
  "license": "Apache-2.0",
  "keywords": [
    "uswds",
    "react",
    "components",
    "comet",
    "open source"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "npx prettier src --write",
    "test": "jest",
    "coverage": "jest --coverage",
    "e2e": "npx cypress open",
    "e2e:format": "npx prettier cypress --write",
    "e2e:ci": "npx cypress run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metrostar/comet-data-viz": "1.1.1",
    "@metrostar/comet-extras": "1.1.0",
    "@metrostar/comet-uswds": "1.4.0",
    "@tanstack/react-query": "4.32.0",
    "@types/keycloak-js": "^3.4.1",
    "@uswds/uswds": "3.6.0",
    "axios": "1.4.0",
    "axios-mock-adapter": "1.21.5",
    "keycloak-js": "^22.0.1",
    "oidc-client-ts": "^2.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.2",
    "react-oidc-context": "^2.2.2",
    "react-router-dom": "6.14.2",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vitejs/plugin-react": "4.0.3",
    "axe-core": "4.7.2",
    "cypress": "13.0.0",
    "cypress-axe": "1.5.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "sass": "1.64.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "4.2.0"
  }
}

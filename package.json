{
  "name": "comet-starter",
  "private": true,
  "version": "1.0.0",
  "description": "React and TypeScript starter app, pre-configured with the Comet component library and other tools.",
  "license": "Apache-2.0",
  "keywords": [
    "uswds",
    "react",
    "components",
    "comet",
    "open source"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "npx prettier src --write",
    "test": "jest",
    "coverage": "jest --coverage",
    "e2e": "npx cypress open",
    "e2e:format": "npx prettier cypress --write",
    "e2e:ci": "npx cypress run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metrostar/comet-data-viz": "1.1.3",
    "@metrostar/comet-extras": "1.3.2",
    "@metrostar/comet-uswds": "2.0.3",
    "@tanstack/react-query": "5.24.1",
    "@tanstack/react-table": "8.11.7",
    "@uswds/uswds": "3.7.1",
    "axios": "1.6.8",
    "axios-mock-adapter": "1.22.0",
    "oidc-client-ts": "^3.0.1",
    "react": "18.2.0",
    "react-avatar": "^5.0.3",
    "react-dom": "18.2.0",
    "react-hook-form": "7.50.1",
    "react-oidc-context": "^3.0.0",
    "react-router-dom": "6.22.3",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@babel/preset-env": "7.24.3",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.19",
    "axe-core": "4.9.0",
    "cypress": "13.7.1",
    "cypress-axe": "1.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "sass": "1.72.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3",
    "vite": "5.2.7",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "4.3.1"
  }
}
